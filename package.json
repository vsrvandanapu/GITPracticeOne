{
  "name": "playwrightproject",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean.report": "rimraf allure-report allure-results && mkdirp allure-report allure-results",
    "generate.report": "npx allure generate ./allure-results --clean",
    "open.report": "npx allure open ./allure-report",
    "pretest": "npm run clean.report",
    "posttest": "npm run generate.report && npm run open.report",
    "execute.test": "npx playwright test tests/salesforcelogintest.spec.ts --project=chromium --headed",
    "test": "npm run execute.test || npm run posttest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.13.10",
    "@types/xlsx": "^0.0.35",
    "allure-commandline": "^2.34.0",
    "allure-playwright": "^3.2.2",
    "csv-parse": "^5.6.0",
    "luxon": "^3.6.1",
    "mkdirp": "^3.0.1",
    "playwright": "^1.53.2",
    "rimraf": "^6.0.1"
  },
  "dependencies": {
    "xlsx": "^0.18.5"
  }
}
